log "$irdistleft" "$drivendist"
followline "wm" @v 0.15 :($irdistleft < .17 )
stop
fwd .5 @v 1.5 @a .1
stop 
turn 90
fwd .2 @v .1
stop
followwall "left" .2 @v 0.1 :($irdistleft > .4 )
stop
fwd .45 @v 0.1
stop
turn 90 @v .1
stop
fwd .6 @v 1.5 @a .1
stop
turn 90 @v .1
stop
fwd .45 @v 0.1
stop
followwall "left" .2  @v 0.1 :($irdistleft > .4 )
stop
fwd .45 @v 0.1
stop
turn 90 @v .1
stop
followline "wm" @v 0.15 :($drivendist > .6 )
